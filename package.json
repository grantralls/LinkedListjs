  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "mocha": "^9.2.0",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.0.5"
